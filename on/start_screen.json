  //---------------------------------------------------------------------------
  // Skins Panel
  //---------------------------------------------------------------------------

  "change_skin_button@common_buttons.light_glyph_button": {
    "$pressed_button_name": "button.menu_skins",
    "size": [ 25, 25 ],
    "anchor_from": "bottom_middle",
    "anchor_to": "bottom_middle",
    "$button_content_offset": [ 1, 2 ],
    "$button_content": "start.change_skin_icon",
    "$pressed_alpha": 0.65
  },

  "skin_viewer_panel": {
    "type": "input_panel",
    "anchor_from": "bottom_middle",
    "anchor_to": "bottom_middle",
    "size": [ "100%", "100% - 24px" ],
    "offset": [ 0, -16 ],
    "gesture_tracking_button": "button.turn_doll",
    "button_mappings": [
      {
        "from_button_id": "button.menu_select",
        "to_button_id": "button.turn_doll",
        "mapping_type": "pressed",
        "button_up_right_of_first_refusal": true
      }
    ],
    "controls": [
      {
        "paper_doll": {
          "type": "custom",
          "renderer": "paper_doll_renderer",
          "layer": 2,
          "anchor_from": "bottom_middle",
          "anchor_to": "bottom_middle",
          "rotation": "gesture_x",
          "size": [ "100%", "100% - 10px" ],
          "offset": [ 0, -22 ],
          "bindings": [
            {
              "binding_type": "view",
              "source_control_name": "viewer_panel",
              "source_property_name": "#gesture_mouse_delta_x",
              "target_property_name": "#gesture_mouse_delta_x"
            }
          ]
        }
      },

      {
        "paper_doll_name_tag": {
          "type": "custom",
          "renderer": "name_tag_renderer",
          "layer": 2,
          "anchor_from": "top_middle",
          "anchor_to": "bottom_middle",
          "size": [ 8, 8 ],
          "bindings": [
            {
              "binding_name": "#playername"
            }
          ]
        }
      }
    ]
  },

  "skin_panel": {
